  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "roots": [
      "app/javascript"
    ],
    "moduleNameMapper": {
      "\\.(png|jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js",
